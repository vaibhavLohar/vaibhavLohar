
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {

        Scanner sc = new Scanner(System.in);

        System.out.print("Enter the number = ");
        int a = sc.nextInt();

        System.out.print("square of " + a + " = " + a*a);
        System.out.print("\ncube of " + a + " = " + a*a*a);
    }
}
